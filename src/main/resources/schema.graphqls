type Query {
    Role(id: ID): Role
    TaxCategory(id: ID): TaxCategory
    DeliveryMethod(id: ID): DeliveryMethod
}

type Role {
    ID: ID
    name: String
}

type TaxCategory {
    ID: ID
    name: String
    taxRate: Int
}

type DeliveryMethod {
    ID: ID
    name: String
    price: Int
    freeFrom: Int
}
